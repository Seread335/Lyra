# Problem 8: Max/Min
proc max(a: i32, b: i32) -> i32 {
    if a > b { return a; }
    return b;
}

proc min(a: i32, b: i32) -> i32 {
    if a < b { return a; }
    return b;
}

print("Problem 8 - Max(45,32):");
let p8a: i32 = max(45, 32);
print(p8a);
print("Problem 8 - Min(45,32):");
let p8b: i32 = min(45, 32);
print(p8b);

# Problem 9: Even/Odd Checkers
proc is_even(n: i32) -> i32 {
    if n % 2 == 0 { return 1; }
    return 0;
}

proc is_odd(n: i32) -> i32 {
    if n % 2 == 1 { return 1; }
    return 0;
}

print("Problem 9 - Even(4):");
let p9a: i32 = is_even(4);
print(p9a);
print("Problem 9 - Odd(7):");
let p9b: i32 = is_odd(7);
print(p9b);

# Problem 10: Count Pattern
proc count_to(n: i32) {
    var i: i32 = 1;
    while i <= n {
        print("Count: ");
        print(i);
        i = i + 1;
    }
}

print("Problem 10 - Count(3):");
count_to(3);

