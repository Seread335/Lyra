{
  "name": "Lyra Light",
  "type": "light",
  "colors": {
    "editor.background": "#ffffff",
    "editor.foreground": "#333333",
    "editor.lineNumberActiveForeground": "#0066cc",
    "editorCursor.foreground": "#0066cc",
    "editor.selectionBackground": "#add6ff",
    "editor.wordHighlightBackground": "#f0f0f0"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#808080",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#ce3262"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#0a7013"
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword.control, keyword.declaration",
      "settings": {
        "foreground": "#0066cc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Built-in Functions",
      "scope": "support.function.builtin",
      "settings": {
        "foreground": "#7f0055",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function Name",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#795e26"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#0066cc"
      }
    },
    {
      "name": "Variables",
      "scope": "variable.other",
      "settings": {
        "foreground": "#333333"
      }
    }
  ]
}
