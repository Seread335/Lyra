print("start");

proc is_prime(n: i32) -> i32 {
    if n < 2 { return 0; }
    var i: i32 = 2;
    while i * i <= n {
        if n % i == 0 { return 0; }
        i = i + 1;
    }
    return 1;
}

print("middle");
let p1: i32 = is_prime(17);
print(p1);
