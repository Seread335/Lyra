print("Test 1: Basic Print");

proc is_prime(n: i32) -> i32 {
    if n < 2 {
        return 0;
    }
    var i: i32 = 2;
    while i * i <= n {
        if n % i == 0 {
            return 0;
        }
        i = i + 1;
    }
    return 1;
}

print("Test 2: Prime Function Defined");

proc factorial(n: i32) -> i32 {
    if n <= 1 {
        return 1;
    }
    return n * factorial(n - 1);
}

print("Test 3: Factorial Function Defined");

proc gcd(a: i32, b: i32) -> i32 {
    if b == 0 {
        return a;
    }
    return gcd(b, a % b);
}

print("All functions defined");
let prime_test: i32 = is_prime(17);
print(prime_test);
