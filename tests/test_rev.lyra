proc reverse_number(n: i32) -> i32 {
    let reversed: i32 = 0;
    let temp: i32 = n;
    while temp > 0 {
        let digit: i32 = temp % 10;
        let reversed: i32 = reversed * 10 + digit;
        let temp: i32 = temp / 10;
    }
    return reversed;
}

let result: i32 = reverse_number(123);
print(result);
